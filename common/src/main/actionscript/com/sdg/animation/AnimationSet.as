package com.sdg.animation{		import com.sdg.animation.sequence.ISequence;		public class AnimationSet	{		protected var sequences:Object = {};		protected var _length:int;				public function AnimationSet()		{			_length = 0;		}				public function addSequence(key:String, sequence:ISequence):void		{			sequences[key] = sequence;			_length++;		}				public function getSequence(key:String):ISequence		{			return sequences[key];		}				public function get length():int		{			return _length;		}    }}