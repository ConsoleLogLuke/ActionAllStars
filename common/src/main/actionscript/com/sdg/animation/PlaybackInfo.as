package com.sdg.animation{		import com.sdg.animation.sequence.ISequence;		public class PlaybackInfo	{		public static const DEFAULT_PLAYBACK_INFO:PlaybackInfo = new PlaybackInfo();				private var _interval:Number;		private var _intervalType:uint;		private var _loopCount:uint;				public function get interval():Number		{			return _interval;		}				public function get intervalType():uint		{			return _intervalType;		}				public function get loopCount():int		{			return _loopCount;		}				public function PlaybackInfo(interval:Number = NaN, loopCount:uint = 0, intervalType:uint = 0)		{			_intervalType = intervalType;			_interval = interval;			_loopCount = loopCount;		}    }}