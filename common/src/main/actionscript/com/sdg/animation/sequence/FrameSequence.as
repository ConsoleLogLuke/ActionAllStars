package com.sdg.animation.sequence{	import com.sdg.animation.PlaybackInfo;	import flash.events.Event;		public class FrameSequence implements ISequence	{		private var _playbackInfo:PlaybackInfo		private var _keyframes:Array = [];				public function get duration():uint		{			return _keyframes.length;		}				public function set duration(value:uint):void		{			_keyframes.length = value;		}				public function get playbackInfo():PlaybackInfo		{			return _playbackInfo;		}				public function FrameSequence(playbackInfo:PlaybackInfo = null)		{			_playbackInfo = playbackInfo;		}				public function addFrameEvent(index:uint, event:Event):void		{			if (!_keyframes[index])				_keyframes[index] = new SequenceFrame();						_keyframes[index].events.push(event);		}				public function getSequenceCursor():ISequenceCursor		{			return new ArraySequenceCursor(_keyframes);		}	}}