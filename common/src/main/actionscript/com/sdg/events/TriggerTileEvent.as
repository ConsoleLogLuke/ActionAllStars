package com.sdg.events{	import flash.events.Event;	public class TriggerTileEvent extends Event	{		public static const TILE_TRIGGER:String = "tileTrigger";				public var params:Object;				public function TriggerTileEvent(type:String, params:Object = null)		{			super(type);			this.params = params;		}				override public function clone():Event		{			return new TriggerTileEvent(type, params);		}	}}