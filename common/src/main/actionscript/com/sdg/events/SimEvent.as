package com.sdg.events{	import flash.events.Event;		public class SimEvent extends Event	{		public static const ADDED:String = 'added';		public static const REMOVED:String = 'removed';		public static const MOTION_ENABLED:String = 'motionEnabled';		public static const MOTION_DISABLED:String = 'motionDisabled';		public static const SIMULATION_FRAME:String = 'simulationFrame';		public static const SIMULATION_START:String = 'simulationStart';		public static const SIMULATION_STOP:String = 'simulationStop';		public static const VALIDATED:String = 'validated';		public static const MOVED:String = 'sim object moved';				public function SimEvent(type:String)		{			super(type);		}				override public function clone():Event		{			return new SimEvent(type);		}	}}