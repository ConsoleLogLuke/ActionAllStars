package com.sdg.view{	import com.sdg.control.GameCastController;	import com.sdg.control.room.RoomController;	import com.sdg.control.room.itemClasses.IRoomItemController;	import com.sdg.display.IInteractiveObject;	import com.sdg.display.IRoomItemDisplay;	import com.sdg.display.render.RenderLayer;		import flash.display.DisplayObject;	import flash.display.Sprite;		import mx.core.IStateClient;	    public interface IRoomView extends IInteractiveObject, IStateClient	{		function get enabled():Boolean;		function set enabled(value:Boolean):void;				function get background():Sprite;		function set background(value:Sprite):void;				function get gameCastController():GameCastController;				function get isBackgroundVisible():Boolean;		function set isBackgroundVisible(value:Boolean):void;				function get uiLayer():Sprite;				function clear():void;				function addRenderLayer(index:uint, renderLayer:RenderLayer):void;				function getRenderLayer(index:uint):RenderLayer;				function getRenderLayerUnderPoint(x:Number, y:Number):RenderLayer;				function addItemDisplay(layer:uint, display:IRoomItemDisplay):void;				function removeItemDisplay(display:IRoomItemDisplay):void;				function addPopUp(popUp:DisplayObject):void;				function removePopUp(popUp:DisplayObject):void;				function centerPopUp(popUp:DisplayObject):void;				function addQuedPopUp(popUp:DisplayObject, transition:String = '', params:Object = null):void;				function removeQuedPopUp(popUp:DisplayObject, transition:String = '', params:Object = null):void;				function setCursor(cursor:DisplayObject):void;				function getRoomController():RoomController;				function set hideCustomCursor(value:Boolean):void;    }}