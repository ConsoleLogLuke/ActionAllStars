package com.sdg.sim.dynamics.integrators{	import com.sdg.sim.dynamics.IIntegrator;	import com.sdg.sim.core.SimData;		public class HeunXY implements IIntegrator	{		private var ode:SimData;				public function HeunXY(ode:SimData)		{			this.ode = ode;		}				public function step(dt:Number):void		{			var dtSq:Number = dt * dt;			var ax:Number;			var ay:Number;						ax = ode.fx * ode.invMass;			ay = ode.fy * ode.invMass;						ode.x += ode.vx * dt + .5 * ax * dtSq;			ode.y += ode.vy * dt + .5 * ay * dtSq;						ode.vx += ax * dt;			ode.vy += ay * dt;		}	}}