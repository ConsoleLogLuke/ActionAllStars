package com.sdg.sim.map{	public class TileGenerator	{		public static function createTileList(list:Array, classMap:Object, tileSetId:String):Array		{			var l:Number = list.length;			var tiles:Array = new Array(l);						for (var i:int = 0; i < l; i++)			{				var classRef:Class = classMap[list[i]];				var tile:IOccupancyTile = (classRef) ? new classRef() : null;				if (!tile) continue;				tile.tileSetID = tileSetId;				tiles[i] = tile;			}						return tiles;		}	}}