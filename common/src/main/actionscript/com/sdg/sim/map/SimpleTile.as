package com.sdg.sim.map{		import com.sdg.sim.entity.IMapOccupant;		public class SimpleTile implements IOccupancyTile	{		public var cost:Number = 0;		protected var occupant:IMapOccupant;		protected var _setID:String;				public function SimpleTile(cost:Number = 0)		{			this.cost = cost;		}				public function getOccupant():IMapOccupant		{			return occupant;		}				public function setOccupant(entity:IMapOccupant):void		{			occupant = entity;		}				public function getTraversalCost(entity:IMapOccupant):Number		{			if (occupant == entity) return 0;			if (occupant == null) return cost;			return occupant.solidity + cost;		}				public function validateOccupancy(entity:IMapOccupant):Boolean		{			return (occupant == null || occupant == entity);		}				public function set tileSetID(value:String):void		{			_setID = value;		}				public function get tileSetID():String		{			return _setID;		}	}}