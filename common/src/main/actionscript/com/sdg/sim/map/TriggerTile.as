package com.sdg.sim.map{		import com.sdg.sim.entity.IMapOccupant;		import flash.events.Event;		public class TriggerTile extends SimpleTile	{		public var event:Event;		public var eventEnabled:Boolean;				public function TriggerTile(cost:Number = 0, event:Event = null)		{			super(cost);			this.event = event;			eventEnabled = true;		}				override public function setOccupant(entity:IMapOccupant):void		{			occupant = entity;			if (occupant != null && eventEnabled == true) occupant.dispatchEvent(event);		}	}}