package com.sdg.sim.render{	import com.sdg.display.render.RenderData;	import com.sdg.geom.AABB;	import com.sdg.sim.core.SimData;	import com.sdg.sim.core.SimEntity;		import flash.display.DisplayObject;		public class SimRenderData extends RenderData	{		protected var _entity:SimEntity;		public var simData:SimData;		public var aabb:AABB;				public function SimRenderData(display:DisplayObject = null):void		{			super(display);		}				public function get entity():SimEntity		{			return _entity;		}		public function set entity(value:SimEntity):void		{			_entity = value;		}	}}