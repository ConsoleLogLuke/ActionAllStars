package com.sdg.ext{	import flash.events.Event;	public class SDGExtEvent extends Event	{		public static const GAME_RESULT:String = 'gameResult';		public static const GAME_RESULT_ERROR:String = 'gameResultError';		public static const GAME_ATTRIBUTES:String = 'gameAttributes';		public static const GAME_ATTRIBUTES_ERROR:String = 'gameAttributesError';				public var data:Object;		public var text:String;				public function SDGExtEvent(type:String, data:Object = null, text:String = null)		{			super(type);						this.data = data;			this.text = text;		}				override public function clone():Event		{			return new SDGExtEvent(type, data, text);		}				override public function toString():String		{			return "[SDGExtEvent type=" + type + ", data=" + data + ", text=" + text + "]";		}	}}