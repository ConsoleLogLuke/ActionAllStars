package com.sdg.geom{	public class RMatrix	{		public var r11:Number = 1;		public var r12:Number = 0;		public var r21:Number = 0;		public var r22:Number = 1;				public function RMatrix(angle:Number = 0)		{			if (angle != 0) orient(angle);		}				public function identity():void		{			r11 = r22 = 1, r12 = r21 = 0;		}				public function orient(angle:Number):void		{			r22 = r11 = Math.cos(angle), r21 = Math.sin(angle), r12 = -r21;		}				public function clone():RMatrix		{			var rm:RMatrix = new RMatrix(0);			rm.r11 = r11, rm.r12 = r12, rm.r21 = r21, rm.r22 = r22;			return rm;		}				public function toString():String		{			return "(r11=" + r11 + ", r12=" + r12 + ", r21=" + r21 + ", r22=" + r22 + ")";		}	}}