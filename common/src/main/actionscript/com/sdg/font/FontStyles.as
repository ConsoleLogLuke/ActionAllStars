package com.sdg.font
{
	import flash.text.StyleSheet;
	
	public class FontStyles extends Object
	{
		public static const GILL_SANS:StyleSheet = new StyleSheet();
		public static const EUROSTYLE:StyleSheet = new StyleSheet();
		GILL_SANS.setStyle('font-family', 'GillSans');
		EUROSTYLE.setStyle('font-family', 'EuroStyle');
		
	}
}