package com.sdg.collections{	public interface IMapCollection extends ICollection	{		/**		 * Adds the item to the map at the specified key		 */		function add(key:Object, value:*):void;				/**		 * Returns the item associated with the specified key.		 */		function get(key:Object):*;				/**		 * Returns an array of keys.		 */		function getKeys():Array;				/**		 * Removes and returns the item at the specified key.		 */		function remove(key:Object):*;		/**		 * Returns the key of the specified item.		 */		function keyOf(value:*):Object;		/**		 * Returns whether the key is found.		 */		function containsKey(key:Object):Boolean;	}}