package com.sdg.collections{	public dynamic class QuickList extends Array	{		public function QuickList(numElements:int = 0):void		{			super(numElements);		}				public function contains(value:*):Boolean		{			return indexOf(value) > -1;		}				public function insert(index:int, value:*):void		{			splice(index, 0, value);		}				public function replace(oldValue:*, value:*):int		{			var index:int = indexOf(oldValue);			if (index > -1) this[index] = value;			return index;		}				public function remove(index:uint):*		{			return splice(index, 1)[0];		}				public function removeValue(value:*):int		{			var index:int = indexOf(value);			if (index > -1) splice(index, 1);			return index;		}				public function shuffle():void		{			sort(randomise);						function randomise(a:Object, b:Object):Number 			{				var num:Number = Math.round(Math.random() * 2 ) - 1;				return num;			}		}				public function toArray():Array		{			return slice();		}	}}