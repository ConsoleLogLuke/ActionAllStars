package com.sdg.display.render{	import flash.display.DisplayObject;		public class RenderObject	{		internal var parent:RenderLayer;				private var _data:RenderData;				public function get data():RenderData		{			return _data;		}				public function get display():DisplayObject		{			return _data.display;		}				public function set display(value:DisplayObject):void		{			_data.display = value;		}				public function get depth():int		{			return _data.depth;		}				public function set depth(value:int):void		{			_data.depth = value;		}				public function get originX():Number		{			return _data.originX;		}				public function set originX(value:Number):void		{			_data.originX = value;		}				public function get originY():Number		{			return _data.originY;		}				public function set originY(value:Number):void		{			_data.originY = value;		}				public function RenderObject(data:RenderData = null):void		{			_data = (data) ? data : new RenderData();		}	}}